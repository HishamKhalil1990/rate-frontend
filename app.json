{
  "expo": {
    "name": "alrayhan",
    "slug": "alrayhan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.abuodehbros.rates",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "40aaeb13-2f42-4646-9cd2-0226d785e1bc"
      }
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow alrayhan to access your camera."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "alrayhan app accesses your photos to let you share them"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow alrayhan to access your photos.",
          "savePhotosPermission": "Allow alrayhan to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
